
&Вместо("ПередЗаписью")
Процедура гф_ПередЗаписью(Отказ, Замещение)
	// #wortmann { 
	// Проверка на заполненность измерения "Документ" при виде данных "Документ" 
	// Галфинд_Домнышева 2023/07/26
	Для Каждого ТекЗапись Из ЭтотОбъект Цикл
		Если ТекЗапись.ВидДанных = Перечисления.гф_ВидыДанныхДляВыгрузки.Документ 
			И НЕ ЗначениеЗаполнено(ТекЗапись.Документ) Тогда
			Попытка	
				МенеджерЗаписи = РегистрыСведений.гф_ОшибкиЗагрузки.СоздатьМенеджерЗаписи(); 				
				МенеджерЗаписи.Дата = ТекущаяДатаСеанса();
				МенеджерЗаписи.ПутьФайла = ТекЗапись.ИмяФайла; 
				МенеджерЗаписи.Номер =  "000";
				МенеджерЗаписи.ТекстОшибки = "Не выполнена запись в РС ""ДанныеВыгрузки"", не заполнено значение ""Документ""";
				МенеджерЗаписи.Интерфейс = Перечисления.гф_Интерфейсы.WMS;
				МенеджерЗаписи.ТипИнформации = "Ошибка";
				МенеджерЗаписи.Записать(); 
				
			Исключение
				СообщениеОбОшибке = "Ошибка при записи файла " + ТекЗапись.ИмяФайла  
				+ ". Описание ошибки: Не удалось загрузить ТекстОшибки в РС гф_ОшибкиЗагрузки. " + ОписаниеОшибки();
				ЗаписьЖурналаРегистрации("РСДанныеВыгрузки", УровеньЖурналаРегистрации.Ошибка, ЭтотОбъект, ЭтотОбъект, СообщениеОбОшибке);		
			КонецПопытки;
			Отказ = Истина;
			Прервать;
		КонецЕсли;
	КонецЦикла;
	// } #wortmann
	ПродолжитьВызов(Отказ, Замещение);
КонецПроцедуры
