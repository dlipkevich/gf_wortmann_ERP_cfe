		<query>ВЫБРАТЬ
	Номенклатура.Ссылка КАК Ссылка,
	ЕСТЬNULL(GTINСвойство.Значение, "") КАК GTIN,
	Номенклатура.Код КАК Код,
	Номенклатура.Артикул КАК Артикул,
	Номенклатура.Наименование КАК Наименование,
	Номенклатура.СтранаПроисхождения КАК СтранаПроисхождения,
	Номенклатура.КодТНВЭД КАК КодТНВЭД,
	Номенклатура.НаименованиеПолное КАК НаименованиеПолное,
	Номенклатура.Марка КАК Марка,
	ЕСТЬNULL(МатериалВерхаСвойствоДС.Значение, "") КАК МатериалВерха,
	ЕСТЬNULL(ВидОбувиСвойство.Значение, "") КАК ВидОбуви,
	ЕСТЬNULL(МатериалПодкладкиСвойство.Значение, "") КАК МатериалПодкладки,
	ЕСТЬNULL(МатериалНизаСвойство.Значение, "") КАК МатериалНиза,
	ЕСТЬNULL(ЦветСвойство.Значение, "") КАК Цвет,
	ЕСТЬNULL(РазмерСвойство.Значение, "") КАК РазмерСвойство,
	ЕСТЬNULL(Организации.ИНН, "") КАК ИННИмпорт,
	Организации.Ссылка КАК ОрганизацияСсылка
ПОМЕСТИТЬ ВТ
ИЗ
	Справочник.Номенклатура КАК Номенклатура
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ДополнительныеСведения КАК РазмерСвойство
		ПО Номенклатура.Ссылка = РазмерСвойство.Объект
			И (РазмерСвойство.Свойство.Имя = "Size")
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ДополнительныеСведения КАК ЦветСвойство
		ПО Номенклатура.Ссылка = ЦветСвойство.Объект
			И (ЦветСвойство.Свойство.Имя = "гф_НоменклатураColor_Name")
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ДополнительныеСведения КАК МатериалНизаСвойство
		ПО Номенклатура.Ссылка = МатериалНизаСвойство.Объект
			И (МатериалНизаСвойство.Свойство.Имя = "гф_НоменклатураMaterialBottom")
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ДополнительныеСведения КАК МатериалПодкладкиСвойство
		ПО Номенклатура.Ссылка = МатериалПодкладкиСвойство.Объект
			И (МатериалПодкладкиСвойство.Свойство.Имя = "гф_НоменклатураMaterialLining")
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ДополнительныеСведения КАК GTINСвойство
		ПО Номенклатура.Ссылка = GTINСвойство.Объект
			И (GTINСвойство.Свойство.Имя = "гф_НоменклатураGLN_Manufacturer")
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ДополнительныеСведения КАК ВидОбувиСвойство
		ПО Номенклатура.Ссылка = ВидОбувиСвойство.Объект
			И (ВидОбувиСвойство.Свойство.Имя = "гф_НоменклатураProduct_Group_Supplier")
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Организации КАК Организации
		ПО (ВЫБОР
				КОГДА ПОДСТРОКА(Номенклатура.Артикул, 1, 4) = "1-1-"
					ТОГДА Организации.ИНН = "7705855199"
				КОГДА ПОДСТРОКА(Номенклатура.Артикул, 1, 4) = "2-2-"
					ТОГДА Организации.ИНН = "7705993537"
				КОГДА ПОДСТРОКА(Номенклатура.Артикул, 1, 4) = "5-5-"
					ТОГДА Организации.ИНН = "7705993544"
				КОГДА ПОДСТРОКА(Номенклатура.Артикул, 1, 4) = "8-8-"
					ТОГДА Организации.ИНН = "7705993551"
				КОГДА ПОДСТРОКА(Номенклатура.Артикул, 1, 4) = "9-9-"
					ТОГДА Организации.ИНН = "7705856435"
				ИНАЧЕ ЛОЖЬ
			КОНЕЦ)
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ДополнительныеСведения КАК МатериалВерхаСвойствоДС
		ПО Номенклатура.Ссылка = МатериалВерхаСвойствоДС.Объект
			И (МатериалВерхаСвойствоДС.Свойство.Имя = "гф_НоменклатураMaterialSurface")
;

