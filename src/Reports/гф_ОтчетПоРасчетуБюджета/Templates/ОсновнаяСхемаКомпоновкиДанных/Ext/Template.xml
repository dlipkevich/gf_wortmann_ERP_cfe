	а.Контрагент КАК Контрагент,
	а.Регистратор.Бюджет КАК Бюджет,
	а.СуммаОборот КАК СуммаУпр,
	а.Организация КАК Организация
{ВЫБРАТЬ
	СтатьяОборотов.*,
	Контрагент.*,
	Бюджет.*,
	СуммаУпр}
ИЗ
	РегистрНакопления.гф_ДвиженияБюджетов.Обороты(, {(&amp;ДатаОкончания)}, Авто, ) КАК а
ГДЕ
	(&amp;НетБюджетов
			ИЛИ а.Регистратор.Бюджет В (&amp;Бюджет))
{ГДЕ
	а.Контрагент.*,
	а.Организация.*}</query>
