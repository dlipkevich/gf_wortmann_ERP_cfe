		//расчет контрольного числа
		ДлинаШтрихкода = СтрДлина(НовыйШтрихКод);  
		
		//правка лысов
		// загрушка, слетает на артикулах кроме ботинкоф
		Если ДлинаШтрихкода = 15 Тогда
			Четные = 0;
