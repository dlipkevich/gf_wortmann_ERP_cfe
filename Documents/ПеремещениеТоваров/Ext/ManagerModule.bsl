			КонецЕсли;
			
			НовыйШтрихКод = НовыйШтрихКод + КонтрольноеЧисло;
		Иначе
			НовыйШтрихКод = Стр.Номенклатура.Код;
		КонецЕсли;
		//КонецЕсли;
		
