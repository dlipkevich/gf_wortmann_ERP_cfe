				Символ = Сред(НовыйШтрихКод,Поз, 1);	  
				Если Цел(Поз/2) * 2 = Поз тогда
					Четные = Четные + Число(Символ);
				Иначе
					Нечетные = Нечетные + Число(Символ);
				КонецЕсли;
			КонецЦикла;
			
			СуммаЧисел = Нечетные * 3 + Четные;  
