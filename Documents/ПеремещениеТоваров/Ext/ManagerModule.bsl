	//ИмяФайла = "invoice_kuchino_po_1_" + СокрЛП(Справочники.B2B_w_Настройки.ОбменДаннымиКодКлиента.Значение) + "_" + СокрЛП(ТекущийДокумент.Номер) + "_1_" + День(ТекущийДокумент.Дата) + "_" + Месяц(ТекущийДокумент.Дата) + "_" + Прав(Год(ТекущийДокумент.Дата),2)+  ".xls";
	//ИмяФайла = "invoice_kuchino" + ?(КодНайден, "_wortmann", "") + "_po_1_" + НомерЗаявки + "_1_" + День(ТекущийДокумент.Дата) + "_" + Месяц(ТекущийДокумент.Дата) + "_" + Прав(Год(ТекущийДокумент.Дата),2)+  ".xls";
